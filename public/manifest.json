{
  "manifest_version": 2,
  "name": "Sked tools by Stiger",
  "version": "1.0",
  "description": "Chrome extension for the Skedulo tool",
  "background": {
    "scripts": []
  },
  "browser_action": {
    "default_title": "Sked tools",
    "default_icon": "icons/logo.png",
    "default_popup": "index.html"
  },
  "incognito": "split",
  "icons": {
    "16": "icons/logo.png",
    "48": "icons/logo.png",
    "128": "icons/logo.png"
  },
  "permissions": [
    "activeTab",
    "http://*/",
    "https://*/"
  ]
}